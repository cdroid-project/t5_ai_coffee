<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:bottom="25dp" android:left="5dp" android:right="5dp" android:top="25dp">
        <shape android:shape="rectangle">
            <corners android:radius="27dp"/>
            <gradient android:startColor="#1922e1ff"
                    android:centerColor="#191d8fe1"
                    android:endColor="#19625eb1"/>
        </shape>
    </item>
    <item android:bottom="25dp" android:left="5dp" android:right="5dp" android:top="25dp">
        <clip>
            <shape android:shape="rectangle">
                <corners android:radius="27dp"/>
                <gradient android:startColor="#FF22e1ff"
                    android:centerColor="#FF1d8fe1"
                    android:endColor="#FF625eb1"/>
            </shape>
        </clip>
    </item>
</layer-list>