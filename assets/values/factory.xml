<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="factory_match_parent">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">#000000</item>
    </style>
    <style name="factory_menu_btn">
        <item name="android:layout_width">240dp</item>
        <item name="android:layout_height">100dp</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:background">@drawable/factory_menu_btn</item>
        <item name="android:textColor">#AF9E82</item>
        <item name="android:textSize">20sp</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="factory_color_plot">
        <item name="android:layout_width">60dp</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:background">@drawable/factory_touch</item>
    </style>
</resources>